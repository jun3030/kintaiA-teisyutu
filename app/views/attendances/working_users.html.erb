<h1>出勤社員一覧</h1>


<table class="table-bordered table-striped table-condensed">
  <thead>
    <tr>
      <th>社員番号</th>
      <th>名前</th>
    </tr>
  </thead>
  <tbody>
    <% @attendance.each do |attendance| %>
      <% User.where(id: attendance.user_id).each do |user|  %>
        <tr>
          <th><%= user.employee_number %></th>
          <th><%= user.name %></th>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>